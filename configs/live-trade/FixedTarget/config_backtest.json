{
    "tag": "bot",
    "log":{
        "level": "DEBUG",
        "file": "log/icarus-backtest.log",
        "clear": false
    },
    "credential_file": "test_credentials.json",
    "mongodb":{
        "database": "FixedTarget_backtest",
        "host": "localhost",
        "port": 27017,
        "clean": true
    },
    "balances": [
        {
            "asset": "USDT", 
            "free": 30000, 
            "locked": 0
        }
    ],
    "broker": {
        "quote_currency": "USDT",
         "fee_rate": 0.001
    },
    "strategy_allocation":{
        "type": "DiscreteStrategyAllocator",
        "kwargs": {
            "distribution_config": ["FixLimAdap_24_24_03-LTCUSDT", "FixLimAdap_24_24_03-XRPUSDT", "FixOCOAdap_24_12_03-DIAUSDT"],
            "capital_coeff":0.9, 
            "stop_capital":null,
            "capital_limit": 150
        }
    },
    "strategy": {
        "FixLimAdap_24_24_03-LTCUSDT": {
            "strategy": "FixedLimitTargetAdaptive",
            "pairs": ["LTCUSDT"],
            "time_scales": ["1h"],
            "max_live_trade": 1,
            "kwargs":{
                "target_profit_coeff":1.03,
                "exit_duration":24,
                "stop_entry_counter":24
            }
        },
        "FixLimAdap_24_24_03-XRPUSDT": {
            "strategy": "FixedLimitTargetAdaptive",
            "pairs": ["XRPUSDT"],
            "time_scales": ["1h"],
            "max_live_trade": 1,
            "kwargs":{
                "target_profit_coeff":1.03,
                "exit_duration":24,
                "stop_entry_counter":24
            }
        },
        "FixOCOAdap_24_12_03-DIAUSDT": {
            "strategy": "FixedOCOTargetAdaptive",
            "pairs": ["DIAUSDT"],
            "time_scales": ["1h"],
            "max_live_trade": 1,
            "kwargs":{
                "target_profit_coeff":1.03,
                "exit_duration":24,
                "stop_entry_counter":12,
                "max_loss_coeff": 0.05
            }
        }
    },
    "analysis": {
        "close":{},
        "aroon":{}
    },
    "time_scales": {
        "1m": 3,
        "15m": 3,
        "1h": 3
    },
    "backtest":{
        "start_time": "2023-4-28 10:40:01",
        "end_time": "2023-5-13 21:00:00",
        "online": true,
        "breakpoints": {}
    },
    "report_folder_name": "reports_backtest",
    "report": [],
    "grid_search_reporters": {
        "grid": {
            "<strategy>": ["FixLimAdap_24_24_03-LTCUSDT", "FixLimAdap_24_24_03-XRPUSDT", "FixOCOAdap_24_12_03-DIAUSDT"],
            "<pair>": ["LTCUSDT", "XRPUSDT", "DIAUSDT"],
            "<start_time>": [1682631000],
            "<end_time>": [1684044000]
        },
        "reporters": [
            {
                "parameters": {
                    "<pair-indice>": ["LTCUSDT", "XRPUSDT", "DIAUSDT"],
                    "<timeframe>": ["1h"],
                    "<analyzer>": ["close"]
                },
                "reporter": "symbol_price_change",
                "source": "analyzer",
                "indice_template": ["<pair-indice>", "<timeframe>", "<analyzer>"],
                "writers": [
                    "markdown_table"
                ]
            },
            {
                "parameters": {},
                "reporter": "strategy_statistics",
                "query_template": [
                    [
                        {
                            "$match":{
                                "strategy": "<strategy>", 
                                "pair": "<pair>",
                                "decision_time": { "$gte": "<start_time>"},
                                "result.exit.time": { "$lte": "<end_time>" }
                                }
                            },
                        {
                            "$project": {
                                "strategy":"<strategy>-<pair>",
                                "status": "$status",
                                "cause": "$result.cause",
                                "exit_type": "$result.exit.type",
                                "profit":"$result.profit",
                                "fee":{"$sum":[ "$result.exit.fee", { "$multiply": [ "$result.enter.fee", "$result.enter.price" ] }]},
                                "duration":"$result.live_time",
                                "is_updated":{"$ne": ["$order_stash", []]},
                                "risk_data": {
                                    "enter_price":"$result.enter.price",
                                    "target_price": "$exit.price",
                                    "stop_limit_price": "$exit.stop_limit_price"
                                    },
                                "price_change":{"$divide":[ { "$subtract": [ "$result.exit.price", "$result.enter.price" ] }, "$result.enter.price"]},
                                "percentage_profit":{"$divide":[ 
                                    "$result.profit", 
                                    {"$multiply": [
                                            "$result.enter.price",
                                            { "$sum": [ "$result.enter.quantity", "$result.enter.fee" ] }
                                        ]}]
                                    }
                        }}
                    ]
                ],
                "source": "database",
                "collection": "hist-trades",
                "writers": [
                    "json_file"
                ]
            },
            {
                "parameters": {},
                "reporter": "trade_cause",
                "query_template": [
                    [
                        {
                            "$match":{
                                "strategy":"<strategy>",
                                "pair": "<pair>",
                                "decision_time": { "$gte": "<start_time>"},
                                "result.exit.time": { "$lte": "<end_time>"}
                            }
                        },
                        {
                            "$project": {
                                "strategy":"<strategy>-<pair>",
                                "cause": "$result.cause"
                            }
                        }
                    ]
                ],
                "source": "database",
                "collection": "hist-trades",
                "writers": [
                    "pie_plot"
                ]
            },
            {
                "parameters": {},
                "reporter": "trade_profit_duration_distribution",
                "query_template": [
                    [
                        {
                            "$match":{
                                "strategy":"<strategy>",
                                "pair": "<pair>",
                                "decision_time": { "$gte": "<start_time>"},
                                "result.exit.time": { "$lte": "<end_time>"}
                            }
                        },
                        {
                            "$project": {
                                "strategy":"<strategy>-<pair>",
                                "profit": "$result.profit",
                                "duration":"$result.live_time"
                            }
                        }
                    ]
                ],
                "source": "database",
                "collection": "hist-trades",
                "writers": [
                    "joint_histogram"
                ]
            },
            {
                "parameters": {},
                "reporter": "strategy_statistics",
                "query_template": [
                    [
                        {
                            "$match":{
                                "decision_time": { "$gte": "<start_time>"},
                                "result.exit.time": { "$lte": "<end_time>"}
                            }
                        },
                        {
                            "$project": {
                                "strategy":"All",
                                "status": "$status",
                                "cause": "$result.cause",
                                "profit":"$result.profit",
                                "fee":{"$sum":[ "$result.exit.fee", { "$multiply": [ "$result.enter.fee", "$result.enter.price" ] }]},
                                "duration":"$result.live_time",
                                "is_updated":{"$ne": ["$order_stash", []]},
                                "price_change":{"$divide":[ { "$subtract": [ "$result.exit.price", "$result.enter.price" ] }, "$result.enter.price"]},
                                "percentage_profit":{"$divide":[ 
                                    "$result.profit", 
                                    {"$multiply": [
                                            "$result.enter.price",
                                            { "$sum": [ "$result.enter.quantity", "$result.enter.fee" ] }
                                        ]}]
                                    }
                            }
                        }
                    ]
                ],
                "source": "database",
                "collection": "hist-trades",
                "writers": [
                    
                ]
            },
            {
                "parameters": {},
                "reporter": "balance_statistics",
                "query_template": [
                    [
                        {
                            "$match":{
                                "type":"quote_asset",
                                "ts": { "$gte": "<start_time>", "$lte": "<end_time>" }
                            }
                        },
                        { "$project" : {
                            "ts": "$ts", 
                            "total": "$data.total"
                            } 
                        }
                    ]
                ],
                "source": "database",
                "collection": "observer",
                "writers": [
                    "json_file"
                ]
            },
            {
                "parameters": {},
                "reporter": "strategy_capitals",
                "query_template": [
                    [
                        {
                            "$match":{
                                "type":"strategy_capitals",
                                "ts": { "$gte": "<start_time>", "$lte": "<end_time>" }
                            }
                        },
                        { "$project" : {
                            "ts": "$ts", 
                            "data": "$data"
                            }
                        }
                    ]
                ],
                "source": "database",
                "collection": "observer",
                "writers": [
                    "line_plot"
                ]
            },
            {
                "parameters": {},
                "reporter": "strategy_capital_statistics",
                "query_template": [
                    [
                        {
                            "$match":{
                                "type":"strategy_capitals",
                                "ts": { "$gte": "<start_time>", "$lte": "<end_time>" }
                            }
                        },
                        {
                            "$group": {
                                "_id": null,
                                "first_capitals": { "$first": "$$ROOT.data" },
                                "last_capitals": { "$last": "$$ROOT.data" }
                              }
                        }
                    ]
                ],
                "source": "database",
                "collection": "observer",
                "writers": [
                    "markdown_table"
                ]
            },
            {
                "parameters": {},
                "reporter": "r_multiples",
                "query_template": [
                    [
                        {
                            "$match":{
                                "result.exit.type":"oco", 
                                "strategy":"<strategy>",
                                "pair": "<pair>",
                                "decision_time": { "$gte": "<start_time>"},
                                "result.exit.time": { "$lte": "<end_time>"}
                            }
                        },
                        { "$project" : {
                            "strategy":"<strategy>-<pair>",
                            "cause":"$result.cause",
                            "duration":"$result.live_time",
                            "enter_price":"$result.enter.price",
                            "target_price":"$exit.price",
                            "stop_limit_price":"$exit.stop_limit_price"
                            }
                        }
                    ]
                ],
                "source": "database",
                "collection": "hist-trades",
                "writers": [
                    "joint_histogram"
                ]
            },
            {
                "parameters": {},
                "reporter": "trade_perc_profit",
                "query_template": [
                    [
                        {
                            "$match":{
                                "strategy":"<strategy>",
                                "decision_time": { "$gte": "<start_time>"},
                                "result.exit.time": { "$lte": "<end_time>"}
                            }
                        },
                        {
                            "$project": {
                                "strategy":"<strategy>-<pair>",
                                "decision_time":"$decision_time",
                                "percentage_profit":{"$divide":[ 
                                    "$result.profit", 
                                    {"$multiply": [
                                            "$result.enter.price",
                                            { "$sum": [ "$result.enter.quantity", "$result.enter.fee" ] }
                                        ]}]
                                    }
                            }
                        }
                    ]
                ],
                "source": "database",
                "collection": "hist-trades",
                "writers": [
                    "histogram"
                ]
            }
        ]
    },
    "visualization":{
        "observers":{
            "quote_asset":["free", "total", "in_trade"]
        },
        "indicators": {
            "close":{}
        },
        "patterns":[]
    }
}